<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Dalamud.NET.Sdk/14.0.2">
    <PropertyGroup>
        <Name>EnhancedBattleMode</Name>
        <Author>NaughtyNeko</Author>
        <Punchline>Intelligent management of draw/sheathe actions for a popular game.</Punchline>
        <Description>Intelligent management of draw/sheathe actions for a popular game.</Description>
        <Version>$(VersionPrefix)</Version>
        <WarningLevel>4</WarningLevel>
        <PackageProjectUrl>https://github.com/NjalTheKnut/EnhancedBattleMode</PackageProjectUrl>
        <PackageLicenseExpression>AGPL-3.0-or-later</PackageLicenseExpression>
        <IsPackable>false</IsPackable>
    </PropertyGroup>
    <ItemGroup>
      <PackageReference Include="ECommons" Version="3.1.0.26" />
    </ItemGroup>
    <ItemGroup>
      <PackageReference Update="DotNet.ReproducibleBuilds" Version="2.0.2" />
    </ItemGroup>
</Project>
